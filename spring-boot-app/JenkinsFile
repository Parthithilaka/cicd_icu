pipeline {
  agent {
    docker {
      image "abhishekf5/maven-abhishek-docker-agent:v1"
    }
  }
  stages {
    stage ('checkout'){
      steps {
        echo "passed"
      }
    }
    stages ('build maven') {
      stage {
        steps {
          sh "ls -lrt"
          sh "cd sprind-boot-app"
          sh "pwd"
          sh "mvn clean  packages"
        }
      }
    }
  }
}