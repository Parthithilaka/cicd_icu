pipeline {
  agent {
    docker { 
      image 'abhishekf5/maven-abhishek-docker-agent:v1'
    }
  }
  stages {
    stage('SCM checkout') {  // Corrected typo here
      steps { 
        sh 'echo passed'
      }
    }
    stage('Build maven') {
      steps {
        sh 'ls -ltr'
        // build the project and create a JAR file
        sh 'cd spring-boot-app && mvn clean package'
      }
    }
  }
}
